<odoo>
    <data>
        <template id="assets_backend" name="vs assets"
            inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript"
                    src="/epc_branding/static/src/js/chrome.js"></script>
            </xpath>
        </template>

        <template id="fav_ico" name="educat Icon" inherit_id="web.layout">
        	<xpath expr="//html/head/title" position="replace">
                <title>EPC</title>
            </xpath>
            <xpath expr="//html/head/link[1]" position="replace">
                <link rel="shortcut icon"
                    href="/epc_branding/static/src/img/epc_favicon.ico"
                    type="image/x-icon" />
            </xpath>
        </template>

    </data>
</odoo>
