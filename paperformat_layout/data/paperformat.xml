<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        
        <record id="paperformat_landscape_gt" model="report.paperformat">
            <field name="name">GT Barcode</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">96</field>
            <field name="page_width">144</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">20</field>
            <field name="margin_bottom">12</field>
            <field name="margin_left">10</field>
            <field name="margin_right">10</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">17</field>
            <field name="dpi">90</field>
        </record>

    </data>
</odoo>