<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="inherited_payslips_views" model="ir.ui.view">
            <field name="name">Inherited HR Payslip</field>
            <field name="model">hr.payslip</field>
            <field name="inherit_id" ref="hr_payroll.view_hr_payslip_form"/>
            <field name="arch" type="xml">
                <xpath expr="/form/header/button[@name='action_payslip_cancel']" position="after">
                    <button name="%(yuko_maternity_report.report_yuko_matermity)d" string="Print Maternity Benefit Bill" type="action"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>