<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="inherit_product_gate_in_form_view" model="ir.ui.view">
            <field name="name">product.gate.in.form.view</field>
            <field name="model">product.gate.in</field>
            <field name="inherit_id" ref="product_gate_in.product_gate_in_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//group/group/field[@name='partner_id']" position="replace">
                    <field name="partner_id" context="{'search_default_supplier':1, 'default_supplier':1, 'default_customer':0}" domain="[('supplier','=',True)]"/>
                    <field name="supplier_phone"/>
                    <field name="gate_entry"/>
                </xpath>
                <xpath expr="//group/group/field[@name='date']" position="replace">
                    <field name="entry_date"/>
                </xpath>
                <xpath expr="//group/group/field[@name='receive_type']" position="before">
                    <field name="responsible"/>
                </xpath>
                <xpath expr="//group/group/field[@name='create_by']" position="after">
                    <field name="carried_phone"/>
                </xpath>
                <xpath expr="//group/group/field[@name='received']" position="replace">
                </xpath>
            </field>
        </record>
    </data>
</odoo>
