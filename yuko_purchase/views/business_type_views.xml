        <field name="type">ir.actions.act_window</field>
        <field name="res_model">business.category.type</field>
        <field name="context">{}</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record><!--
    <menuitem id="menu_business_category_type_form_action" parent="purchase.menu_purchase_config"
